[init]
    defaultBranch = main
[user]
    name = at337am
    email = lueqit@gmail.com
[alias]
    co = checkout
    br = branch
    ci = commit
    sw = switch
    st = status
    lg = log --oneline --graph --decorate --all
    last = log -1 HEAD
    count = rev-list --count HEAD
    wip = commit -m "WIP"
    rth = reset --hard
    rts = reset --soft
    pom = push origin main
    aw = !git add --all && git commit -m "WIP"
    res = restore
